{
    "manifest_version": 2,
    "name": "ChatGPT Plugin Profiles",
    "version": "1.0",
    "description": "Easily manage ChatGPT plugin profiles",
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_popup": "public/index.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://chat.openai.com/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
  